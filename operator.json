{
  "name": "Downsample",
  "description": "Downsizes or balances a dataset.",
  "tags": [
    "preprocessing"
  ],
  "authors": [
    "tercen"
  ],
  "urls": [
    "https://github.com/tercen/downsample_operator"
  ],
  "properties": [
    {
      "kind": "DoubleProperty",
      "name": "seed",
      "defaultValue": -1,
      "description": "Seed for random number generation. If -1, a random seed is taken."
    }
  ],
  "operatorSpec": {
    "kind": "OperatorSpec",
    "ontologyUri": "https://tercen.com/_ontology/tercen",
    "ontologyVersion": "0.0.1",
    "inputSpecs": [
      {
        "kind": "CrosstabSpec",
        "metaFactors": [
          {
            "kind": "MetaFactor",
            "name": "Observation ID",
            "type": "",
            "description": "Observation identifiers (events, ...)",
            "ontologyMapping": "observation",
            "crosstabMapping": "column",
            "cardinality": "1..n",
            "factors": []
          },
          {
            "kind": "MetaFactor",
            "name": "Color",
            "type": "",
            "description": "Grouping factor for downsampling (optional)",
            "ontologyMapping": "color",
            "crosstabMapping": "color",
            "cardinality": "0..1",
            "factors": []
          }
        ],
        "axis": []
      }
    ]
  }
}